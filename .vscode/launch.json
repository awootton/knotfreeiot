    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
    
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "main.go ",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": [
               "--nano",
            ]
        },
        {
            "name": "startCluster.go ",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/clusterInProcess/startCluster.go",
            "args": [
               "--nano",
            ]
        }
    ]
}